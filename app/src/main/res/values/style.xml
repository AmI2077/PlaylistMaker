<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="headerTextStyle">
        <item name="fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">22sp</item>
        <item name="fontWeight">500</item>
        <item name="android:letterSpacing">0</item>
    </style>

    <style name="mainMenuBtnStyle">
        <item name="backgroundTint">@color/white</item>
        <item name="cornerRadius">16dp</item>
        <item name="fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">22sp</item>
        <item name="fontWeight">500</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textColor">@color/yp_black</item>
        <item name="android:textAllCaps">false</item>
        <item name="iconTint">@color/yp_black</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">8dp</item>
    </style>

    <style name="editTextStyle">
        <item name="hintMaxLines">1</item>
        <item name="fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="fontWeight">400</item>
        <item name="android:inputType">text</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:textCursorDrawable">@drawable/edit_text_cursor</item>
    </style>

    <style name="regularTextStyle">
        <item name="android:textSize">16sp</item>
        <item name="fontFamily">@font/ys_display_regular</item>
        <item name="fontWeight">400</item>
        <item name="android:letterSpacing">0</item>
    </style>

    <style name="regularTextStyle.11">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/yp_text_gray</item>
    </style>



</resources>